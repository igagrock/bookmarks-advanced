{

  "description": "app",
  "manifest_version": 2,
  "name": "demo",
  "version": "1.0",

  "permissions" : [
    "tabs", "<all_urls>","bookmarks","notifications" 
  ],

  "background" :{
    "scripts" : ["js/background-bundle.js"]
  },
 
  "browser_action": {
    "default_icon": {
      "16": "icons/main-16.png",
      "32": "icons/main-32.png"
    }
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js; object-src 'self'"
   ,
   "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}